{
	"manifest_version": 2,
	"name": "Grab Session Links",
	"version": "0.2",
	"content_scripts": [
		{
		"matches": ["<all_urls>"],
		"js": ["content.js"]
		}
	],
	"permissions": [
		"tabs"
	],
	"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icons/save16.png",
		"32": "icons/save32.png",
		"128": "icons/save128.png",
		"256": "icons/save256.png"
	}
}
